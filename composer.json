{
    "name": "vendor/project",
    "type": "project",
    "description": "My project",
    "license": "",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "ergebnis/composer-normalize": false,
            "phpro/grumphp": true
        }
    },
    "require": {
        "php": ">7.3",
        "ext-ctype": "*",
        "ext-iconv": "*"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "friends-of-phpspec/phpspec-code-coverage": "^6.0",
        "nikic/php-parser": "^4.10",
        "pestphp/pest": "^1.0",
        "phpro/grumphp": "^1.3",
        "phpspec/phpspec": "^7.0",
        "phpunit/phpunit": "^9.5",
        "phpunitgen/console": "^1.3",
        "phpunitgen/core": "^1.1",
        "sebastian/phpcpd": "^6.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
