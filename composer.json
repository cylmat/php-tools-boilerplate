{
    "name": "vendor/project",
    "type": "project",
    "description": "My project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpro/grumphp": true
        }
    },
     "require": {
        "php": "^7"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
